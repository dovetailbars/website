{
  "trailingSlash": "auto",
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/images/*",
      "/public/*"
    ]
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'",
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS"
  },
  "routes": [
    {
      "route": "/bookings/*",
      "methods": [
        "get",
        "post"
      ],
      "statusCode": 200
    }
  ]
}