{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/*",
        "methods": ["GET", "HEAD", "POST", "OPTIONS"]
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*", "/public/*"]
    },
    "globalHeaders": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, OPTIONS"
    }
}